<?xml version="1.0" encoding="UTF-8"?>
<config>
  <propel>
    <datasources default="Propel">
      <datasource id="Propel">
        <adapter>mysql</adapter> <!-- sqlite, mysql, mssql, oracle, or pgsql -->
        <connection>
          <dsn>mysql:host=localhost;dbname=S4A4_2</dsn>
          <user>root</user>
          <password>root</password>
            <settings>
                <setting id="charset">utf8</setting>
            </settings>
            <options>
                <option id="MYSQL_ATTR_INIT_COMMAND">SET NAMES utf8 COLLATE utf8_unicode_ci</option>
            </options>
        </connection>
      </datasource>
    </datasources>
  </propel>
</config>

